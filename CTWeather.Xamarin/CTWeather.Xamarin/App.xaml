             xmlns:local="clr-namespace:CTWeather.Xamarin"
             x:Class="CTWeather.Xamarin.App">
    <Application.Resources>
        <ResourceDictionary>
            <FontImageSource x:Key="weatherCloud" Color="White" Size="20" FontFamily="FontAwesomeBold" Glyph="{x:Static local:Icon.Cloud}"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>